{
  "name": "I'm Feeling Blue",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Verification for the rest of us",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "injectScript.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}